end})

Tab1:AddSlider({
  Name = "Walkspeed",
  Min = 1,
  Max = 100,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
  end
})

Tab1:AddSlider({
  Name = "JumpPower",
  Min = 1,
  Max = 100,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
  game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
  end
})

Tab1:AddSlider({
  Name = "FOV",
  Min = 70,
  Max = 120,
  Increase = 1,
  Default = 70,
  Callback = function(Value)
  game.Workspace.CurrentCamera.FieldOfView = Value
  end
})

